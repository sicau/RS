<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="cn.edu.sicau.rs.bean">
	
	<class name="User" table="tb_user">
		<id name="id">
			<generator class="native"/>
		</id>
		
		<property name="sigNumber" column="signumber"/>
		
		<property name="userName" column="username"/>
		
		<property name="password"/>
		
		<property name="trueName" column="truename"/>
		
		<property name="testNumber" column="testnumber"/>
		
		<property name="sex"/>
		
		<property name="birthday"/>
		
		<property name="politics"/>
		
		<property name="identity"/>
		
		<property name="subject"/>
		
		<property name="school"/>
		
		<property name="phone"/>
		
		<property name="mphone"/>
		
		<property name="sa"/>
		
		<property name="spostcode"/>
		
		<property name="ha"/>
		
		<property name="hpostcode"/>
		
		<property name="lang"/>
		
		<property name="category"/>
		
		<property name="prize"/>
		
		<property name="speciality"/>
		
		<property name="src"/>
		
		<property name="type"/>
		
	</class>
	
</hibernate-mapping>